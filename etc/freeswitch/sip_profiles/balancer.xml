<profile name="balancer">

  <aliases>
  </aliases>

  <domains>
    <domain name="tgx.sippulse.com" alias="false" parse="true"/>
    <domain name="api4com.com" alias="false" parse="true"/>
    <domain name="174.138.32.248" alias="false" parse="true"/>
    <domain name="sip.pstnhub.microsoft.com" alias="false" parse="true"/>
  </domains>

  <settings>
    <param name="sip-port" value="5070"/>
    <param name="user-agent-string" value="SipPulse TGW"/>
    <param name="disable-transcoding" value="false"/>
    <param name="inbound-late-negotiation" value="true"/>
    <param name="disable-transfer" value="false"/>
    <param name="disable-hold" value="true"/>
    <param name="enable-timer" value="false"/>
    <param name="debug" value="0"/>
    <param name="sip-trace" value="no"/>
    <param name="sip-capture" value="no"/>
    <param name="rfc2833-pt" value="101"/>
    <param name="dialplan" value="XML"/>
    <param name="context" value="default"/>
    <param name="dtmf-duration" value="2000"/>
    <param name="inbound-codec-prefs" value="OPUS,PCMU"/>
    <param name="outbound-codec-prefs" value="OPUS,PCMU"/>
    <param name="hold-music" value="$${hold_music}"/>
    <param name="rtp-timer-name" value="soft"/>
    <param name="local-network-acl" value="localnet.auto"/>
    <!--<param name="apply-inbound-acl" value="peers"/>-->
    <param name="manage-presence" value="true"/>
    <param name="presence-hold-state" value="confirmed"/>
    <param name="inbound-codec-negotiation" value="generous"/>
    <param name="nonce-ttl" value="60"/>
    <param name="auth-calls" value="true"/>
    <param name="accept-blind-auth" value="false"/>
    <param name="log-auth-failures" value="true"/>
    <param name="inbound-late-negotiation" value="false"/>
    <param name="inbound-zrtp-passthru" value="false"/> 
    <param name="rtp-ip" value="174.138.32.248"/>
    <param name="sip-ip" value="174.138.32.248"/>
    <param name="ext-sip-ip" value="174.138.32.248"/>
    <param name="ext-rtp-ip" value="174.138.32.248"/> 
    <param name="rtp-timeout-sec" value="300"/>
    <param name="rtp-hold-timeout-sec" value="1800"/>
    <param name="tls" value="false"/>
    <param name="tls-only" value="false"/>
    <param name="tls-bind-params" value="transport=tls"/>
    <param name="tls-sip-port" value="5061"/>
    <param name="tls-cert-dir" value="/usr/local/freeswitch/conf/tls"/>
    <param name="tls-passphrase" value=""/>
    <param name="tls-verify-date" value="true"/>
    <param name="tls-verify-policy" value="none"/>
    <param name="tls-verify-depth" value="2"/>
    <param name="tls-verify-in-subjects" value=""/>
    <param name="tls-version" value="$${sip_tls_version}"/>
  </settings>
</profile>
